<table id="search-series" class="table table-striped">
  <tr>
    <th></th>
    <th>Title</th>
    <th></th>
  </tr>
  <% @series.each do |serie| %>
    <tr>
      <td><img class="serie-thumbnail" src="<%= serie.picture_url %>"/></td>
      <td><%= serie.title %></td>
      <td><button type="button" class="btn btn-primary">Track</button></td>
    </tr>
  <% end %>
</table>
